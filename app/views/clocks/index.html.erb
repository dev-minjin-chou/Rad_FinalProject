<p id="notice"><%= notice %></p>

<h1>Clocks</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>Abbr</th>
      <th>Offset</th>
      <th>Isdst</th>
      <th>Text</th>
      <th>Utc</th>
      <th>City</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clocks.each do |clock| %>
      <tr>
        <td><%= clock.value %></td>
        <td><%= clock.abbr %></td>
        <td><%= clock.offset %></td>
        <td><%= clock.isdst %></td>
        <td><%= clock.text %></td>
        <td><%= clock.utc %></td>
        <td><%= clock.city %></td>
        <td><%= link_to 'Show', clock %></td>
        <td><%= link_to 'Edit', edit_clock_path(clock) %></td>
        <td><%= link_to 'Destroy', clock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clock', new_clock_path %>
